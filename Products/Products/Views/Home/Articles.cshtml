@model Products.Models.Articles
@{
    ViewBag.Title = "Articles";
}

<h2>Articles</h2>

<div class="container">
    <div class="col-md-6 col-md-offset-3">
        <ul>
            @foreach (var art in Model.articles)
            {
                <li>@art.ArticleName</li>
                <li>@art.ArticleDescription</li>
                <li>@art.ArticleCategory</li>
                <li>@art.ArticleManufacturer</li>
                <li>@art.ArticleSupplier</li>
                <li>@art.ArticlePrice</li>
            }
        </ul>
    </div>
</div>	